@model IEnumerable<aspNETuniversity.Models.Faculty>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
@using (Html.BeginForm())
{
    <p>
        Find: @Html.TextBox("SearchString")  
        <input type="submit" value="Search" /></p>
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeanName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FacultyName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeanName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FacultyName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FacultyCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FacultyCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FacultyCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
